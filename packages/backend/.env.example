# Server Configuration
PORT=3001
NODE_ENV=development

# JWT Secret
JWT_SECRET=your-secret-key-change-in-production

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/prostor

# Storage Configuration
# Options: local, s3, r2
STORAGE_TYPE=local
STORAGE_BUCKET=prostor-files
STORAGE_REGION=auto
STORAGE_ENDPOINT=https://your-endpoint.com
STORAGE_ACCESS_KEY_ID=your-access-key-id
STORAGE_SECRET_ACCESS_KEY=your-secret-access-key

# Search Configuration
# Options: meilisearch, typesense
SEARCH_TYPE=meilisearch
SEARCH_HOST=http://localhost:7700
SEARCH_API_KEY=your-search-api-key